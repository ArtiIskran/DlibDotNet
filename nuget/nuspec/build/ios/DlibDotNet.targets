<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <ItemGroup Condition="(('$(Platform)' == 'iPhone'))">
        <NativeReference Include="$(MSBuildThisFileDirectory)ios-device\DlibDotNetNative.framework">
            <Kind>Framework</Kind>
            <ForceLoad>True</ForceLoad>
            <SmartLink>True</SmartLink>
            <IsCxx>True</IsCxx>
            <LinkerFlags>-lc++</LinkerFlags>
        </NativeReference>
        <NativeReference Include="$(MSBuildThisFileDirectory)ios-device\DlibDotNetNativeDnn.framework">
            <Kind>Framework</Kind>
            <ForceLoad>True</ForceLoad>
            <SmartLink>True</SmartLink>
            <IsCxx>True</IsCxx>
            <LinkerFlags>-lc++</LinkerFlags>
        </NativeReference>
    </ItemGroup>
    <ItemGroup Condition="(('$(Platform)' == 'iPhoneSimulator'))">
        <NativeReference Include="$(MSBuildThisFileDirectory)ios-simulator\DlibDotNetNative.framework">
            <Kind>Framework</Kind>
            <ForceLoad>True</ForceLoad>
            <SmartLink>True</SmartLink>
            <IsCxx>True</IsCxx>
            <LinkerFlags>-lc++</LinkerFlags>
        </NativeReference>
        <NativeReference Include="$(MSBuildThisFileDirectory)ios-simulator\DlibDotNetNativeDnn.framework">
            <Kind>Framework</Kind>
            <ForceLoad>True</ForceLoad>
            <SmartLink>True</SmartLink>
            <IsCxx>True</IsCxx>
            <LinkerFlags>-lc++</LinkerFlags>
        </NativeReference>
    </ItemGroup>
